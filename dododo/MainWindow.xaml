<Window x:Class="dododo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dododo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="112*"/>
            <RowDefinition Height="67*"/>
            <RowDefinition Height="66*"/>
            <RowDefinition Height="189*"/>
        </Grid.RowDefinitions>
        <TextBox x:Name="Test" Margin="269,20,269,19" Grid.Row="2"/>
        <TextBox x:Name="uhg" Margin="269,13,269,27" Text="{Binding Test}" Grid.Row="1" TextChanged="uhg_TextChanged"/>
    </Grid>
</Window>
